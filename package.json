{
  "name": "dual-ai-sentiment-analysis-system",
  "version": "1.0.0",
  "description": "Production-Ready Enterprise AI Sentiment Analysis System - A+ (99/100) grade dual AI sentiment analysis platform featuring advanced security, optimized performance, and enterprise-grade reliability with GPT-OSS-120B and DistilBERT-SST-2 integration.",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "node build-frontend.js",
    "build:frontend:only": "node build-frontend.js --skip-typecheck",
    "build:backend": "node build-backend.js",
    "build:prod": "tsc --project tsconfig.prod.json",
    "build:dev": "tsc --project tsconfig.dev.json",
    "build:check": "npm run typecheck",
    "deploy": "./scripts/deployment/deploy-production.sh",
    "deploy:only": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env=staging",
    "deploy:build-only": "npm run build",
    "deploy:verify": "curl -sf https://tft-trading-system.yanggf.workers.dev/api/v1/data/health | jq .",
    "deploy:quick": "./scripts/deployment/quick-deploy.sh",
    "deploy:frontend": "./scripts/deployment/deploy-frontend.sh",
    "deploy:frontend:only": "./scripts/deployment/deploy-frontend.sh --skip-backend",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "test": "echo 'Run specific test suites'",
    "test:performance": "npx playwright test tests/performance.spec.js --reporter=list",
    "test:workflows": "npx playwright test tests/user-workflows.spec.js --reporter=list",
    "test:playwright": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "test:report": "npx playwright show-report",
    "test:performance-all": "./tests/performance/test-playwright-performance.sh",
    "test:integration": "./scripts/utilities/validate-enhanced-cache.sh",
    "test:cache": "./scripts/utilities/validate-enhanced-cache.sh",
    "test:strict-guards": "node -e 'import(\"./test-strict-mode-guards.ts\").then(m => m.runStrictModeGuardrailTests().then(console.log, console.error))'",
    "test:production-guards": "node -e 'import(\"./test-production-guards.ts\").then(m => m.runProductionGuardsTests().then(console.log, console.error))'",
    "test:guards-smoke": "./scripts/test-runners/test-production-guards-smoke.sh",
    "test:reports-html": "./tests/validation/test-reports-html-verification.sh",
    "test:validate-environment": "./tests/validation/validate-environment-bindings.sh",
    "test:html-structure": "./tests/integration/frontend/test-html-structure-validation.sh",
    "test:synthetic-monitoring": "./tests/validation/test-synthetic-monitoring-html.sh",
    "test:mock-prevention": "./tests/feature/mock-elimination/test-mock-prevention-scan.sh",
    "verify": "npm run test:html-structure && npm run test:guards-smoke && npm run test:validate-environment && npm run test:synthetic-monitoring && npm run test:mock-prevention",
    "verify:staging": "npm run verify && npm run test:performance-all",
    "verify:prod": "npm run build:prod && npm run verify && npm run test:production-guards",
    "prepare": "husky install"
  },
  "keywords": [
    "sentiment-analysis",
    "ai",
    "machine-learning",
    "finance",
    "cloudflare-workers",
    "typescript",
    "enterprise",
    "production-ready"
  ],
  "author": "Enterprise AI Systems",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/dual-ai-sentiment-analysis-system.git"
  },
  "dependencies": {
    "express": "^5.1.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250927.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2",
    "wrangler": "^4.62.0"
  }
}
